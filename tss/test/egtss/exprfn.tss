.listThick:not(.active) {
    background: -webkit-gradient( linear,
                                  left top,
                                  left bottom, color-stop(0%,#f1f1f1),
                                  color-stop(30%,#ffffff)
                                );
    background: -moz-linear-gradient( top, #f1f1f1 0%, #ffffff 30% );
    filter: progid.DXImageTransform.Microsoft.gradient(
                                  startColorstr='#f1f1f1',
                                  endColorstr='#ffffff'
                                );
    margin : tss_add(20, 30)px;
}
