a {
  color1 : 10, 20;
  color2 : 10.1*20px 10.1-20em, 10.1+20ex;
  color3 : 10*tss_add(20,30);
  color4 : 1px 'solid'*2 gray, 1px 2*"solid" gray;
  color5 : 10+20 *30 -40/50.0;
  color6 : (10+20) * (30-40)/50;
  color7 : -20;
  color8 : -20*30;
  color10 : 20 -+10;
  color11 : 1px solid gray, 10px * 40em;
  margin-top: expression(0 - parseInt(offsetHeight / 2) + (documentElement && scrollTop || scrollTop) + 'px');
  filter: DXImageTransform.Microsoft.gradient(startColorstr='#003366', endColorstr='#024587');
  margin: 3px + 4px auto;
}

p {
  width: 1in + 8pt;
}
