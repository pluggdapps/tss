$width = '1000px';
p {
  font: 10px/8px;               /* Plain CSS, no division */
  width: ($width/2);            /* Uses parantheses, does division */
  height: (500px/2);            /* Uses parentheses, does division */
  margin-left: 5px + (8px/2px); /* '+' doesn't use paranthesis, plain CSS  */
}
